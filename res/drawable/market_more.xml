<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <bitmap android:gravity="center" android:src="@drawable/main_more_press" android:tileMode="disabled" />
    </item>
    <item android:state_focused="true">
        <bitmap android:gravity="center" android:src="@drawable/main_more_press" android:tileMode="disabled" />
    </item>
    <item android:state_selected="true">
        <bitmap android:gravity="center" android:src="@drawable/main_more_press" android:tileMode="disabled" />
    </item>
    <item>
        <bitmap android:gravity="center" android:src="@drawable/main_more_normal" android:tileMode="disabled" />
    </item>
</selector>